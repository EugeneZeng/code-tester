<div class="container">
    <script src="/javascripts/jquery.smartAppender.js" type="text/javascript"></script>
    <script src="/javascripts/custom.js" type="text/javascript"></script>
    <h1>{{title}}</h1>
    <div class="panel panel-default">
      <div class="panel-heading">Welcome to {{title}}</div>
      <div class="panel-body" style="height: 300px" id="scrollBody">
        <ul id="testField" class="nav nav-tabs nav-stacked">
        </ul>
      </div>
      <div class="panel-body" style="height: 300px" id="scrollBody">
        <h4 id="onScrollStart"><span class="label label-default">onScrollStart</span></h4>
        <h4 id="onScroll"><span class="label label-default">onScroll</span></h4>
        <h4 id="onTotalScroll"><span class="label label-default">onTotalScroll</span></h4>
        <h4 id="onTotalScrollBack"><span class="label label-default">onTotalScrollBack</span></h4>
        <h4 id="whileScrolling"><span class="label label-default">whileScrolling</span></h4>
        <div>
            <button class="btn btn-primary" type="button">
              mcs-top <span id="mcs-top" class="badge">4</span>
            </button>
            <button class="btn btn-primary" type="button">
              mcs-dragger-top <span id="mcs-dragger-top" class="badge">4</span>
            </button>
            <button class="btn btn-primary" type="button">
              mcs-top-pct <span id="mcs-top-pct" class="badge">4</span>
            </button>
            <button class="btn btn-primary" type="button">
              mcs-direction <span id="mcs-direction" class="badge">4</span>
            </button>
            <button class="btn btn-primary" type="button">
              mcs-total-scroll-offset <span id="mcs-total-scroll-offset" class="badge">4</span>
            </button>
            <button class="btn btn-primary" type="button">
              mcs-total-scroll-back-offset <span id="mcs-total-scroll-back-offset" class="badge">4</span>
            </button>
        </div>
      </div>
      <div class="panel-footer">
        <div id="operations" class="btn-group" role="group" aria-label="Operations">
            <button type="button" name="getNewest" class="btn btn-default">Get Newest</button>
        </div>
      </div>
    </div>
</div>